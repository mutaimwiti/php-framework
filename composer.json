{
    "name": "mutaimwiti/vanilla-php",
    "description": "Vanilla PHP",
    "type": "project",
    "license": "MIT",
    "require": {
        "ext-json": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": ["app/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2"
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit --bootstrap=vendor/autoload.php tests"
    }
}
