{
    "name": "mutaimwiti/php-framework",
    "authors": [
        {
            "name": "Mutai Mwiti"
        }
    ],
    "description": "PHP Framework",
    "license": "MIT",
    "require": {
        "ext-json": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "src/Framework/"
        },
        "files": ["src/Framework/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2"
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit --bootstrap=vendor/autoload.php tests"
    }
}
